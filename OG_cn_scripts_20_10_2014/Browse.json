      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='intro-inner']/a"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "选项 1"
    },
    {
      "type": "verifyTextPresent",
      "text": "选项 2"
    },
    {
      "type": "verifyTextPresent",
      "text": "选项 3"
    },
    {
      "type": "verifyTextPresent",
      "text": "查看房地产名单"
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='optionbg']/ul/li[5]/a/img"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
