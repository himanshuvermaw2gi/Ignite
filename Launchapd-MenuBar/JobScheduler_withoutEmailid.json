{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": "qa_tester"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "Where2getit!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
    {
      "type": "pause",
      "waitTime": "7000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Reputation\"]"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Job Scheduler"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "useDBFilterNo"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "useDefaultRadius"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//table[@id='sched_table']/tbody/tr[5]/td[3]"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "useDefaultRadius"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "formclear"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "useDBFilterNo"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "useDefaultRadius"
      }
    }
  ]
}
