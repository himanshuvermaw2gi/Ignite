{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://investors.homeunion.com"
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='intro-inner']/a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-link"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-username-input"
      },
      "text": "demo@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-password-input"
      },
      "text": "123456"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-btn"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@class='page-sidebar-menu']//span[normalize-space(.)='myVault']"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "verifyTextPresent",
      "text": "INVESTMENT"
    },
    {
      "type": "verifyTextPresent",
      "text": "CURRENT HOME"
    },
    {
      "type": "verifyTextPresent",
      "text": "PERSONAL DOCUMENTS"
    },
    {
      "type": "pause",
      "waitTime": "8000"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='page-content']/div/div[1]/div/div/div[2]/div/img"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "WATCH TUTORIAL"
    },
    {
      "type": "verifyTextPresent",
      "text": "NEED HELP?"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
