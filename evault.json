{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://192.168.10.13"
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-link"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-username-input"
      },
      "text": "demo@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-password-input"
      },
      "text": "123456"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-btn"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@class='page-sidebar-menu']//span[normalize-space(.)='myVault']"
      }
    },
    {
      "type": "pause",
      "waitTime": "8000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='evCabinet']//a[.='Current Home']"
      }
    },
    {
      "type": "pause",
      "waitTime": "8000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='evCabinet']//a[.='Personal Documents']"
      }
    },
    {
      "type": "pause",
      "waitTime": "7000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//li[@class='start']/a/span"
      }
    },
    {
      "type": "pause",
      "waitTime": "7000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='header-inner']/ul/li[2]/a/span[1]"
      }
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logout-link"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": []
}
