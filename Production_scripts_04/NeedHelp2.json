{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://investor.homeunion.com"
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='intro-inner']/a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-link"
      }
    },
    {
      "type": "pause",
      "waitTime": "1000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-username-input"
      },
      "text": "demo@panalinks.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-password-input"
      },
      "text": "123456"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "id",
        "value": "login-remember-input"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-btn"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='page-content']/div/div[1]/div/div/div[2]/a[2]"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='needhelp']//a[.='Leave a message']"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "html/body/div[4]/div/div[2]/div/div/div[1]/div/div/div[2]/a[2]"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='needhelp']//a[.='Live chat now']"
      }
    },
    {
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='header-inner']/ul/li[2]/a/span[2]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logout-link"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
